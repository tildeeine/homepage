{
  "name": "homepage",
  "version": "1.0.0",
  "description": "Personal portfolio project",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "remark . --quiet --frail --output"
  },
  "remarkConfig": {
    "settings": {
      "emphasis": "*",
      "strong": "*"
